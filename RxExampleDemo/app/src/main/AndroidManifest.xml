<?xml version="1.0" encoding="utf-8"?>
<manifest package="example.rx.com.rxexampledemo"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".butterknife.ButterKnifeSampleActivity"/>
        <activity android:name=".databinding.DataBindingSampleActivity"/>
        <activity android:name=".databinding.DataBindingActivity"/>
        <activity android:name=".databinding.UpdateUserActivity"/>
        <activity android:name=".databinding.ConvertActivity"/>
    </application>

</manifest>